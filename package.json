{
  "name": "express-typescript-seed",
  "version": "1.0.0",
  "private": false,
  "author": "Kevin Kolz <kckolz@gmail.com>",
  "scripts": {
    "start": "yarn run build && yarn run serve",
    "test": "yarn run build && rimraf tests/sqlite.db && NODE_ENV=test jest --forceExit --verbose dist/tests/*.test.js",
    "build": "yarn run clean && tsc",
    "serve": "tsc-watch --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "yarn run build && yarn run serve-debug",
    "serve-debug": "tsc-watch --outDir ./dist --onSuccess \"node --inspect=9222 --trace-warnings ./dist/app.js\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "amqplib": "^0.5.3",
    "aws-sdk": "^2.382.0",
    "bcrypt": "3.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-ensure-login": "^0.1.1",
    "connect-roles": "^3.1.2",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-limiter": "^1.6.1",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^3.0.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-http-oauth": "^0.1.3",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.4.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "sequelize": "^4.42.0",
    "sequelize-typescript": "^0.6.6",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.119",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.18",
    "@types/passport": "^0.4.7",
    "@types/request": "^2.48.1",
    "@types/sequelize": "^4.27.33",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "passport-debug": "^0.1.15",
    "sqlite3": "^4.0.4",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
